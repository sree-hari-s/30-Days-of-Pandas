    "import pandas as pd\n",
    "\n",
    "def count_rich_customers(store: pd.DataFrame) -> pd.DataFrame:\n",
    "    return pd.DataFrame(\n",
    "        store[store['amount'] > 500][['customer_id']].nunique(),\n",
    "        columns=['rich_count'],\n",
    "    )"
